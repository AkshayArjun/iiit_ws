<!-- Rotor joints -->
  <joint name="rotor_0_joint" type="revolute">
    <parent link="base_link"/>
    <child link="rotor_0"/>
    <origin xyz="0.174 0.174 0.06" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1e+16" upper="1e+16" effort="10.0" velocity="100.0"/>
  </joint>
  <joint name="rotor_1_joint" type="revolute">
    <parent link="base_link"/>
    <child link="rotor_1"/>
    <origin xyz="-0.174 0.174 0.06" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1e+16" upper="1e+16" effort="10.0" velocity="100.0"/>
  </joint>
  <joint name="rotor_2_joint" type="revolute">
