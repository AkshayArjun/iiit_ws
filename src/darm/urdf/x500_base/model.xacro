<!-- ARM  -->
  <link name="arm_link">
    <visual>
      <geometry>
          <box size="0.03 0.03 0.002"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <geometry>
         <box size="0.03 0.03 0.002"/>
      </geometry>
    </collision>
    <inertial>
        <mass value="0.100"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <xacro:box_inertia m="0.1" x="0.03" y="0.03" z="0.002"/>
    </inertial>
  </link>
  <joint name="arm_base_fixed_joint" type="fixed">
    <parent link="base_link"/>
    <child link="arm_link"/>
    <origin xyz="0 0 0" rpy="${pi} 0 0"/>
  </joint>
  <link name="link1">
      <visual>
          <geometry>
              <cylinder length="0.0666" radius="0.00666"/>
          </geometry>
          <material name="black">
              <color rgba="0.0 0.0 0.0 1"/>
          </material>
      </visual>
      <inertial>
          <mass value="0.1"/>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <xacro:cylinder_inertia m="0.1" r="0.00666" h="0.0666"/>
      </inertial>
  </link>
    <!-- Joint1: placed at the end of arm_link (z = 0.002/2 = 0.001) -->
  <joint name="joint1" type="revolute">
      <parent link="arm_link"/>
      <child link="link1"/>
      <origin xyz="0 0 0.033" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="-3.14" upper="3.14" effort="1" velocity="1"/>
  </joint>

  <xacro:arm_joint_control joint_name="joint1" link_name="link1"/>
  <link name="link_sphere">
      <visual>
          <geometry>
              <sphere radius="0.00666"/>
          </geometry>
          <material name="black">
              <color rgba="0 0 0 1"/>
          </material>
      </visual>
      <inertial>
          <mass value="0.1"/>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <xacro:sphere_inertia m="0.1" r="0.00666"/>
      </inertial>
  </link>
    <!-- Joint2: placed at the end of link1 (z = 0.0666/2 = 0.0333) -->
  <joint name="joint2" type="revolute">
      <parent link="link1"/>
      <child link="link_sphere"/>
      <origin xyz="0 0 0.0333" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <limit lower="-3.14" upper="3.14" effort="1" velocity="1"/>
  </joint>
  <xacro:arm_joint_control joint_name="joint2" link_name="link_sphere"/>
  <link name="link2">
      <visual>
          <geometry>
              <cylinder length="0.05" radius="0.00433"/>
          </geometry>
          <material name="green">
              <color rgba="0.2 0.8 0.2 1"/>
          </material>
      </visual>
      <inertial>
          <mass value="0.1"/>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <xacro:cylinder_inertia m="0.1" r="0.00433" h="0.05"/>
      </inertial>
  </link>
    <!-- Joint3: placed at the end of link_sphere (z = 0.00666) -->
  <joint name="joint3" type="fixed">
      <parent link="link_sphere"/>
      <child link="link2"/>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
  </joint>

<!-- control tags -->
  <ros2_control name="GazeboSimSystem" type="system">
    <hardware>
        <plugin>gz_ros2_control/GazeboSimSystem</plugin>
    </hardware>

    <!-- chose this for effort controller -->

    <!-- <joint name = "joint1">
      <command_interface name = "effort"/>
      <state_interface name = "position"/>
      <state_interface name = "velocity"/>
      <state_interface name = "effort"/>
    </joint>
    <joint name = "joint2">
      <command_interface name = "effort"/>
      <state_interface name = "position"/>
      <state_interface name = "velocity"/>
      <state_interface name = "effort"/>
    </joint> -->

    <!-- this for position controller -->
    
    <joint name = "joint1">
      <command_interface name = "position"/>
      <state_interface name = "position"/>
    </joint>
    <joint name = "joint2">
      <command_interface name = "position"/>
      <state_interface name = "position"/>
    </joint>
  </ros2_control>

</robot>